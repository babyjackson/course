/* pages/components/studio/detail/detail.wxss */
.h-20{
  height: 20rpx;
}
.h-85{
  height: 85rpx;
}
.xian-2{
  height: 50rpx;
  width: 2rpx;
  background: #d2d2d2;
  margin-top: 15rpx;
}
.stuban{
  position: relative;
  height: 225rpx;
  width: 750rpx;
}
.stuban .banbg{
  width: 750rpx;
  height: 225rpx;
}
.stuban .banbg image{
  display: block;
  width: 750rpx;
  height: 225rpx;
}
.stuban .ban{
  position: absolute;
  top: 0;
  left: 0;
  width: 750rpx;
  height: 225rpx;
}
.stuban .ban .input{
  width: 720rpx;
  height: 55rpx;
  border-radius: 30rpx;
  overflow: hidden;
  background: #ffffff;
  margin: 0 auto;
}
.stuban .ban .input input{
  display: block;
  width: 500rpx;
  height: 55rpx;
  border: 0;
  outline: 0;
  font-size: 21rpx;
  color: #7c7c7c;
}
.stuban .ban .figure{
  display: block;
  width: 100rpx;
  height: 100rpx;
  margin: 0 15rpx;
}
.stuban .ban .figure image{
  display: block;
  border-radius: 10rpx;
  width: 100rpx;
  height: 100rpx;
}
.stuban .ban .dl{
  width: 450rpx;
  height: 100rpx;
}
.stuban .ban .dl .tit{
  height: 55rpx;
  width: 450rpx;
  line-height: 55rpx;
  font-size: 28rpx;
  color: #ffffff;
}
.stuban .ban .dl .dd{
  height: 25rpx;
}
.stuban .ban .dl .glance{
  font-size: 21rpx;
  color: #ffffff;
  line-height: 25rpx;
}
.stuban .ban .dl .glance .ico{
  width: 30rpx;
  height: 25rpx;
}
.stuban .ban .dl .glance .ico image{
  display: block;
  width: 30rpx;
  height: 25rpx;
}
.stuban .ban .dl .coll{
  font-size: 21rpx;
  color: #ffffff;
  line-height: 25rpx;
  margin-left: 25rpx;
}
.stuban .ban .dl .coll .ico{
  width: 30rpx;
  height: 25rpx;
}
.stuban .ban .dl .coll .ico image{
  display: block;
  width: 30rpx;
  height: 25rpx;
}
.stuban .ban .sc{
  width: 120rpx;
  height: 45rpx;
  font-size: 21rpx;
  color: #ffffff;
  line-height: 45rpx;
  border-radius: 20rpx;
  background: linear-gradient(to right,#1bd9cd,#1ecbda);
  margin-top: 65rpx;
  margin-right: 15rpx;
}
.stuban .ban .sc .ico{
  width: 25rpx;
  height: 25rpx;
  margin: 15rpx 8rpx 15rpx 20rpx;
}
.stuban .ban .sc .ico image{
  display: block;
  width: 25rpx;
  height: 25rpx;
}
/***导航**/
.nav{
  display: flex;
  width: 750rpx;
  height: 74rpx;
}
.nav .item{
  flex: 1;
  height: 70rpx;
  border-bottom: 4rpx solid #fff;
  font-size: 28rpx;
  color: #818181;
  text-align: center;
  line-height: 70rpx;
}
.nav .item.active{
  color: #1bd9cd;
  border-bottom: 4rpx solid #1bd9cd;
}
.con-item{
  display: none;
}
.con-item.on{
  display: block;
}
.iban{
  width: 750rpx;
  height: 480rpx;
}
.iban .ban{
  width: 750rpx;
  height: 480rpx;
}
.iban .ban .item{
  width: 750rpx;
  height: 480rpx;
}
.iban .ban .item image{
  display: block;
  width: 750rpx;
  height: 480rpx;
}
/***今日必推**/
.retoday{
  width: 720rpx;
  height: 150rpx;
  background: #c5f6f3;
  padding: 30rpx 15rpx;
}
.retoday .re{
  width: 720rpx;
  height: 150rpx;
  background: #fff;
  border-radius: 20rpx;
  box-shadow: 0rpx 2rpx 15rpx rgba(42, 52, 51, 0.1);
  overflow: hidden;
}
.retoday .re .figure{
  width: 120rpx;
  height: 120rpx;
  margin: 15rpx 20rpx;
}
.retoday .re .figure image{
  display: block;
  width: 120rpx;
  height: 120rpx;
}
.retoday .re .wp{
  width: 500rpx;
  height: 120rpx;
  margin: 15rpx 0;
  overflow: hidden;
}
.retoday .re .wp .tips{
  width: 115rpx;
  height: 35rpx;
  line-height: 35rpx;
  font-size: 24rpx;
  color: #ffffff;
  letter-spacing: 1rpx;
  text-align: center;
  background: #1bdacd;
  border-radius: 10rpx;
}
.retoday .re .wp .text{
  height: 75rpx;
  margin-top: 10rpx;
  line-height: 35rpx;
  font-size: 24rpx;
  color: #5e5e5e;
  width: 500rpx;
  overflow: hidden;
}
/***2***/
.retwo{
  padding: 35rpx 15rpx 0 15rpx;
}
.retwo .item{
  width: 700rpx;
  height: 290rpx;
  border:10rpx solid #1bdacd;
  background: #fff;
  border-radius: 35rpx;
  margin-bottom: 30rpx;
}
.retwo .item:nth-of-type(odd) .figure{
  float: left;
}
.retwo .item:nth-of-type(odd) .dl{
  float: right;
  margin-right: 20rpx;
}
.retwo .item:nth-of-type(even) .figure{
  float: right;
}
.retwo .item:nth-of-type(even) .dl{
  float: left;
  margin-left: 20rpx;
}
.retwo .item .figure{
  width: 250rpx;
  height: 250rpx;
  border-radius: 20rpx;
  margin: 20rpx;
}
.retwo .item .figure image{
  display: block;
  width: 250rpx;
  height: 250rpx;
  border-radius: 20rpx;
}
.retwo .item .dl{
  width: 380rpx;
  height: 250rpx;
  margin: 20rpx 0;
}
.retwo .item .dl .tit{
  width: 380rpx;
  height: 170rpx;
  font-size: 30rpx;
  color: #5e5e5e;
  line-height: 50rpx;
  overflow: hidden;
}
.retwo .item .dl .dd{
  height: 60rpx;
  margin-top: 20rpx;
}
.retwo .item .dl .dd .price{
  font-size: 36rpx;
  color: #1bdacd;
  line-height: 60rpx;
  font-weight: bold;
}
.retwo .item .dl .dd .go{
  width: 125rpx;
  height: 45rpx;
  line-height: 45rpx;
  border-radius: 22rpx;
  background: #1bdacd;
  font-size: 24rpx;
  text-align: center;
  color: #ffffff;
  box-shadow:-2rpx 0 30rpx rgba(27, 218, 205, 0.5);
}
/****热门课程***/
.hotcourse{
  background: #1bdacd;
  border-radius: 30rpx 30rpx 0 0;
}
.hottit{
  height:95rpx;
  padding: 0 20rpx;
}
.hottit .tit{
  font-size: 48rpx;
  color: #ffffff;
  line-height: 95rpx;
  font-weight: bold;
}
.hottit .more{
  width: 500rpx;
  height: 100rpx;
}
.hottit .more image{
  display: block;
  width: 500rpx;
  height: 100rpx;
}
.course{
  padding: 0 20rpx 25rpx 20rpx;
}
.course .item{
  width: 345rpx;
  height: 460rpx;
  border-radius: 25rpx;
  overflow: hidden;
  background: #fff;
  margin-top: 25rpx;
  box-shadow: 2rpx 2rpx 15rpx rgba(4, 32, 31, 0.1);
}
.course .item:nth-of-type(odd){
  float: left;
}
.course .item:nth-of-type(even){
  float: right;
}
.course .item .figure{
  width: 345rpx;
  height: 345rpx;
}
.course .item .figure image{
  display: block;
  width: 345rpx;
  height: 345rpx;
}
.course .item .tit{
  padding: 0 15rpx;
  height: 60rpx;
  text-align: center;
  overflow: hidden;
  font-size: 24rpx;
  color: #5e5e5e;
  line-height: 60rpx;
}
.course .item .price{
  font-size: 36rpx;
  color: #1bdacd;
  line-height: 40rpx;
  text-align: center;
  font-weight: bold;
}
/**底部**/
.btnfixed{
  width: 750rpx;
  height: 85rpx;
  background: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
}
.btnfixed .item{
  width: 374rpx;
  height: 85rpx;
  overflow: hidden;
  font-size: 27rpx;
  color: #676767;
  line-height: 85rpx;
}
.btnfixed .item .ico{
  width: 40rpx;
  height: 40rpx;
  margin: 20rpx 10rpx 20rpx 100rpx; 
}
.btnfixed .item .ico image{
  display: block;
  width: 40rpx;
  height: 40rpx;
}

